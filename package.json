{
  "name": "moco-node",
  "version": "1.1.9",
  "description": "moco stub server for node",
  "main": "dist/index.js",
  "repository": "https://github.com/uuau99999/moco-node.git",
  "author": "yehao <uuau99999@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "node-jswiremock": "^0.0.9"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "lint-staged": {
    "*.{ts}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}